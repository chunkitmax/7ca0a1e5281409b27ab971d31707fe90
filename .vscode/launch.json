  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}"
    },
    {
      "name": "Statistics",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/statistics.py",
    },
    {
      "name": "Train M2O",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "args": [
        "M2O",
        "--gpu",
        "--tensorboard",
        "--data_file_count=3"
      ]
    },
    {
      "name": "Train M2M",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "args": [
        "M2M",
        "--gpu",
        "--tensorboard",
        "--data_file_count=10",
        "--batch_size=10"
      ]
    },
    {
      "name": "Train M2M Test",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "args": [
        "M2M",
        "--gpu",
        "--tensorboard",
        "--data_file_count=3",
        "--batch_size=10",
        "--test"
      ]
    },
    {
      "name": "Train M2O Test",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/main.py",
      "args": [
        "M2O",
        "--gpu",
        "--tensorboard",
        "--data_file_count=3",
        "--test"
      ]
    }
  ]
}